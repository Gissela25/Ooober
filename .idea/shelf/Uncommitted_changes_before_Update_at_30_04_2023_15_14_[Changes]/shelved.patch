+package com.ooober.driver.models
+
+import com.beust.klaxon.*
+private val klaxon = Klaxon()
+
+data class Booking (
+    val id: String? = null,
+    val idClient: String? = null,
+    val idDriver: String? = null,
+    val origin: String? = null,
+    val destination: String? = null,
